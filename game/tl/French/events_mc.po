# game/events_mc.rpy:130 #-#-# translate crowdin pcDream1_457c6f63:
msgid "[_mcthink_] ..."
msgstr "[_mcthink_]..."

# game/events_mc.rpy:131 #-#-# translate crowdin pcDream1_af7212d6:
