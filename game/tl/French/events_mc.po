# game/events_mc.rpy:75 #-#-# translate crowdin kitchenBreakfastDay0_72a33094:
msgid "[_e_] ..."
msgstr "[_e_]..."

# game/events_mc.rpy:76 #-#-# translate crowdin kitchenBreakfastDay0_bf799cd3:
msgid "[_mcthink_] Damn, she doesn't talk. I keep forgetting that..."
