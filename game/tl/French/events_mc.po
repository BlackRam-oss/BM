# game/events_mc.rpy:136 #-#-# translate crowdin pcDream1_8f03e192:
msgid "[_n_] You close your eyes and fall asleep."
msgstr "[_n_] Vous fermez les yeux et vous dormez."

# game/events_mc.rpy:138 #-#-# translate crowdin pcDream1_d0053a27:
