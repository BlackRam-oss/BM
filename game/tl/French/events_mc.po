# game/events_mc.rpy:97 #-#-# translate crowdin kitchenBreakfastDay0_af24eb22:
msgid "[_mcthink_] I mean, she's just a normal girl...right?"
msgstr "[_mcthink_] Je veux dire, elle est juste une fille normale ... pas vrai?"

# game/events_mc.rpy:98 #-#-# translate crowdin kitchenBreakfastDay0_cbb22fe6:
msgid "[_mcthink_] Aside from not talking..."
msgstr "[_mcthink_] En plus de ne pas parler ..."

# game/events_mc.rpy:99 #-#-# translate crowdin kitchenBreakfastDay0_ffeb442f:
