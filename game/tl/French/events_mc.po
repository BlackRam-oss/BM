# game/events_mc.rpy:77 #-#-# translate crowdin kitchenBreakfastDay0_2b35b6fe:
msgid "[_mc_] ..."
msgstr "[_mc_]..."

# game/events_mc.rpy:78 #-#-# translate crowdin kitchenBreakfastDay0_72a33094_1:
